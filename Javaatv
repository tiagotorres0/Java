 questão 12

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Digite um valor:");
    Double num1 = sc.nextDouble();
    System.out.println("Digite outro valor:");
    Double num2 = sc.nextDouble();
    Double restodivi = num1 % num2;
    System.out.println("a divisão de " + num1 + " com " + num2 + " é." + restodivi);
    
    sc.close();
  }
}
============================================================
 questão 13

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Digite seu nome:");
    String nome = sc.nextLine();
    System.out.println("Digite a primeira nota:");
    Double nota1 = sc.nextDouble();
    System.out.println("Digite a segunda nota:");
    Double nota2 = sc.nextDouble();
    Double media = (nota1  + nota2) /2;
    System.out.println("o aluno " + nome + " obteve a media " + media);
    
    sc.close();
  }
}
============================================================
 questão 14

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Digite o primeiro salario:");
    Double salario1 = sc.nextDouble();
    System.out.println("Digite o segundo salario:");
    Double salario2 = sc.nextDouble();
    System.out.println("Digite o terceiro salario:");
    Double salario3 = sc.nextDouble();
    Double media = (salario1  + salario2 + salario3) /3;
    System.out.println(" o salario medio da empresa é " + media);
    
    sc.close();
  }
}

=============================================================
 questão 16

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Digite a primeira nota:");
    Double nota1 = sc.nextDouble();
    System.out.println("Digite a segunda nota:");
    Double nota2 = sc.nextDouble();
    Double media = (nota1 + nota2) / 2;
    if (media<=6.0){
      System.out.println("o aluno foi Reprovado");
    }else{
      System.out.println(" o aluno foi aprovado");
    }
    
    sc.close();
  }
}
=================================
 questão 17

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Digite a temperatura de ontem:");
    Double temp1= sc.nextDouble();
    System.out.println("Digite a temperatura de hoje:");
    Double temp2 = sc.nextDouble();
    if((temp1<20) &&(temp2<20)){
      System.out.println("Vai chover");
    }else if((temp1<20) &&(temp2>20)){
      System.out.println("Não vai chover");
    }else if((temp1>20) &&(temp2<20)){
      System.out.println("Vai chover");
    }else if((temp1>20) &&(temp2>20)){
      System.out.println("Vai chover");
    }
    sc.close();
  }
}
