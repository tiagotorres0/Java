 questão 7

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Digite seu nome:");
    String nome = sc.nextLine();
    System.out.println("Você é de que sexo?:");
    String sexo = sc.nextLine();
    System.out.println("Qual o seu estado civil?:");
    String ec = sc.nextLine();
    System.out.println("Qual a sua idade?:");
    int idade = sc.nextInt();
    System.out.println("Qual o seu salario?:");
    Double salario = sc.nextDouble();
    System.out.println("funcionario " + nome +" do sexo " + sexo + " idade " + idade +" e estado civil " + ec + " recebe " + salario + " de salario.");
    
    sc.close();
  }
}
