  public static void main(String[] args) {
    String nome = "tiago";
    int salario = 2000;
    String sexo = "masculino";
    int idade = 18;
    String ec = "solteiro";
    System.out.println("o funcionario "+ nome +" do sexo "+ sexo +" idade "+idade+" e estado civil "+ ec +" recebe "+ salario +" de salario");
  }
}
