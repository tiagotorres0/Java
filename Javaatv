    Double num1 = sc.nextDouble();
    System.out.println("Digite outro valor:");
    Double num2 = sc.nextDouble();
    Double divi = num1 / num2;
    System.out.println("a divisão de " + num1 + " com " + num2 + " é." + divi);
    
    sc.close();
  }
}
